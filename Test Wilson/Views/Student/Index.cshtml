@model List<Student>
<table class="table">
    <thead>
        <tr>
            <th scope="col">First Name</th>
            <th scope="col">Last Name</th>
            <th scope="col">Faculty</th>
            <th scope="col">Section</th>
            <th scope="col">Roll No</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var studentList in Model)
        {
        <tr>
            <td scope="col"><a href="@Url.Action("Profile", "Student", new {studentId = studentList.Id})" class="font-weight-bold text-primary">@studentList.FirstName</a></td>
            <td scope="col">@studentList.LastName</td>
            <td scope="col">@studentList.Faculty</td>
            <td scope="col">@studentList.Section</td>
            <td scope="col">@studentList.RollNo</td>
        </tr>
        }
    </tbody>
</table>
